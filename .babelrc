{
  "presets": [
    "babel-preset-expo"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-resolver",
          {
            "cwd": "babelrc",
            "root": [
              "./src"
            ],
            "alias": {
              "assets": "./assets"
            }
          }
        ]
      ]
    }
  }
}

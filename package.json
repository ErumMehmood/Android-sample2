{
  "name": "expo-and-typescript",
  "version": "0.0.0",
  "description": "Showcase of an Expo app coded in TypeScript.",
  "author": {
    "email": "jan@aagaard.net",
    "name": "Jan Aagaard"
  },
  "private": true,
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "yarn run start --android",
    "check-types": "tsc",
    "ios": "yarn run start --ios",
    "lint": "eslint --format unix \"**/*.ts\" \"**/*.tsx\"",
    "start": "yarn install && expo start",
    "validate": "yarn install --frozen-lockfile && yarn run lint && yarn run check-types"
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1.1"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "expo": "^41.0.1",
    "expo-analytics-amplitude": "~10.1.0",
    "expo-apple-authentication": "~3.1.0",
    "expo-asset": "~8.3.2",
    "expo-av": "~9.1.2",
    "expo-barcode-scanner": "~10.1.2",
    "expo-blur": "~9.0.3",
    "expo-camera": "~11.0.3",
    "expo-constants": "~10.1.3",
    "expo-facebook": "~11.0.5",
    "expo-font": "~9.1.0",
    "expo-linear-gradient": "~9.1.0",
    "expo-local-authentication": "~11.0.2",
    "expo-permissions": "~12.0.1",
    "expo-sensors": "~10.1.2",
    "react": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-maps": "0.27.1",
    "react-native-reanimated": "~2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.0.0",
    "react-native-svg": "12.1.0"
  },
  "devDependencies": {
    "@types/react": "~16.9.56",
    "@types/react-native": "~0.63.52",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-preset-expo": "8.3.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "expo-cli": "^4.5.2",
    "prettier": "^2.3.1",
    "typescript": "~4.0.8"
  }
}

    "@types/react": "16.8.25",
    "@types/react-native": "0.60.6",
    "@types/react-navigation": "3.0.8",
    "@unimodules/core": "3.0.2",
    "babel-preset-expo": "5.2.0",
    "expo-cli": "3.0.9",
    "prettier": "1.18.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",

{
  "name": "expo-and-typescript",
  "version": "0.0.0",
  "description": "Showcase of an Expo app coded in TypeScript.",
  "author": {
    "email": "jan@aagaard.net",
    "name": "Jan Aagaard"
  },
  "private": true,
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "yarn run start --android",
    "check-types": "tsc",
    "ios": "yarn run start --ios",
    "lint": "eslint --format unix \"**/*.ts\" \"**/*.tsx\"",
    "prettify": "prettier --write \"{./,./src/**/}*.{js,json,ts,tsx}\"",
    "start": "yarn install && expo start",
    "validate": "yarn install && yarn run lint && yarn run check-types"
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1.1"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.8.2",
    "@react-navigation/stack": "^5.11.1",
    "expo": "^39.0.3",
    "expo-analytics-amplitude": "~8.3.1",
    "expo-asset": "~8.2.0",
    "expo-av": "~8.6.0",
    "expo-barcode-scanner": "~9.0.0",
    "expo-blur": "~8.2.0",
    "expo-brightness": "~8.3.0",
    "expo-camera": "~9.0.0",
    "expo-constants": "~9.2.0",
    "expo-facebook": "~9.0.0",
    "expo-font": "~8.3.0",
    "expo-linear-gradient": "~8.3.1",
    "expo-local-authentication": "~9.3.0",
    "expo-permissions": "~9.3.0",
    "expo-sensors": "~9.1.0",
    "react": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.3.tar.gz",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-maps": "0.27.1",
    "react-native-reanimated": "~1.13.1",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-svg": "12.1.0",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@types/expo__vector-icons": "^9.0.1",
    "@types/react": "~16.9.55",
    "@types/react-native": "~0.63.30",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "@unimodules/core": "~5.5.1",
    "babel-preset-expo": "^8.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "expo-cli": "^3.28.2",
    "prettier": "^2.1.2",
    "typescript": "~3.9.7"
  }
}

    "expo-blur": "6.0.0",
    "expo-brightness": "6.0.0",
    "expo-camera": "6.0.0",
    "expo-constants": "6.0.0",
    "expo-core": "3.0.1",
    "expo-font": "6.0.1",
    "expo-linear-gradient": "6.0.0",
    "expo-local-authentication": "6.0.0",
    "expo-permissions": "6.0.0",
    "expo-sensors": "6.0.0",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",

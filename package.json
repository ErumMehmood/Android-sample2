{
  "name": "expo-and-typescript",
  "version": "0.0.0",
  "description": "React Native using Expo and TypeScript",
  "author": {
    "email": "jan@aagaard.net",
    "name": "Jan Aagaard"
  },
  "private": false,
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "check-types": "tsc",
    "ios": "expo start --ios",
    "lint": "tslint --project tsconfig.json",
    "prettify": "prettier --write '{./,./src/**/}*.{js,json,ts,tsx}'",
    "start": "expo start",
    "validate": "yarn run lint && yarn run check-types"
  },
  "dependencies": {
    "expo": "32.0.6",
    "expo-core": "3.0.1",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-navigation": "3.9.1",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/expo": "32.0.13",
    "@types/expo__vector-icons": "9.0.1",
    "@types/react": "16.8.17",
    "@types/react-native": "0.57.55",
    "@types/react-navigation": "3.0.7",
    "babel-preset-expo": "5.1.1",
    "expo-cli": "2.18.5",
    "prettier": "1.17.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.5"
  }
}

{
  "name": "expo-and-typescript",
  "version": "0.0.0",
  "description": "React Native using Expo and TypeScript",
  "author": {
    "email": "jan@aagaard.net",
    "name": "Jan Aagaard"
  },
  "private": true,
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "yarn run start --android",
    "check-types": "tsc",
    "ios": "yarn run start --ios",
    "lint": "tslint --project tsconfig.json",
    "prettify": "prettier --write \"{./,./src/**/}*.{js,json,ts,tsx}\"",
    "start": "yarn install && expo start",
    "validate": "yarn run lint && yarn run check-types"
  },
  "dependencies": {
    "expo": "33.0.7",
    "expo-analytics-amplitude": "5.0.1",
    "expo-asset": "5.0.1",
    "expo-av": "5.0.2",
    "expo-barcode-scanner": "5.0.1",
    "expo-blur": "5.0.1",
    "expo-brightness": "5.0.1",
    "expo-camera": "5.0.1",
    "expo-constants": "5.0.1",
    "expo-core": "3.0.1",
    "expo-font": "5.0.1",
    "expo-linear-gradient": "5.0.1",
    "expo-local-authentication": "5.0.1",
    "expo-permissions": "5.0.1",
    "expo-sensors": "5.0.1",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-maps": "0.24.2",
    "react-native-svg": "9.5.1",
    "react-navigation": "3.11.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@types/expo__vector-icons": "9.0.1",
    "@types/react": "16.8.23",
    "@types/react-native": "0.60.0",
    "@types/react-navigation": "3.0.7",
    "@unimodules/core": "2.0.1",
    "babel-preset-expo": "5.2.0",
    "expo-cli": "2.21.2",
    "prettier": "1.18.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0",
    "typescript": "3.5.2"
  }
}
